{:main [{:des "right_shift + left_shift to caps_lock"
         :rules [[{:key :left_shift
                  :modi {:mandatory [:right_shift]
                        :optional [:caps_lock]}}
                 :caps_lock]
                [{:key :right_shift
                  :modi {:mandatory [:left_shift]
                        :optional [:caps_lock]}}
                 :caps_lock]]}
        
        {:des "caps_lock to left_control"
         :rules [[:caps_lock :left_control]]}]}
