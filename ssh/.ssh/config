# Global settings for all hosts
Host *
  UseKeychain yes
  AddKeysToAgent yes

# GitHub with default key
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519
  
# GitHub with your second key
Host github-jonfk
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519_jonfk

Host nr200u.jonfk.internal
  HostName nr200u.jonfk.internal
  User jonfk
  IdentityFile ~/.ssh/id_ed25519_jonfk

Host ds1019.jonfk.internal
  SetEnv TERM=xterm-256color

Host ds218.jonfk.internal
  SetEnv TERM=xterm-256color
